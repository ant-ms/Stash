{
    "name": "stash",
    "version": "0.0.1",
    "scripts": {
        "dev": "source ../.env && vite dev",
        "build": "svelte-kit sync && vite build",
        "start": "vite",
        "check": "svelte-check --tsconfig ./jsconfig.json",
        "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
        "format": "pnpm run format:prettier && pnpm run format:stylelint",
        "format:prettier": "npx prettier . --write",
        "format:stylelint": "npx stylelint --fix **/*.{css,scss,svelte}"
    },
    "devDependencies": {
        "@egjs/grid": "^1.17.1",
        "@ianvs/prettier-plugin-sort-imports": "^4.6.2",
        "@mdi/js": "^7.4.47",
        "@prisma/adapter-pg": "^7.1.0",
        "@sveltejs/adapter-node": "^5.3.1",
        "@sveltejs/kit": "^2.33.0",
        "@sveltejs/vite-plugin-svelte": "^6.1.2",
        "@types/fuzzy-search": "^2.1.5",
        "@types/mime-types": "^2.1.4",
        "@types/turndown": "^5.0.5",
        "eslint-plugin-svelte": "3.11.0",
        "hash-wasm": "^4.12.0",
        "js-pick-file": "^1.0.13",
        "node-html-parser": "^7.0.1",
        "postcss": "^8.5.6",
        "postcss-html": "^1.8.0",
        "prettier-plugin-svelte": "^3.4.0",
        "pretty-bytes": "^7.0.1",
        "prisma": "^7.1.0",
        "sass": "^1.90.0",
        "stylelint": "^16.23.1",
        "stylelint-config-clean-order": "^7.0.0",
        "stylelint-config-recommended-scss": "^16.0.0",
        "stylelint-config-standard": "^39.0.0",
        "stylelint-scss": "^6.12.1",
        "svelte": "5.38.2",
        "svelte-check": "^4.3.1",
        "svelte-loading-spinners": "^0.3.6",
        "svelte-preprocess": "^6.0.3",
        "ts-exif-parser": "^0.2.2",
        "typescript": "^5.9.2"
    },
    "type": "module",
    "dependencies": {
        "@mistralai/mistralai": "^1.9.17",
        "cookie": "^1.0.2",
        "fuse.js": "^7.1.0",
        "fuzzy-search": "^3.2.1",
        "mdi-svelte": "^1.1.2",
        "mime-types": "2.1.35",
        "redis": "^5.8.1",
        "runed": "^0.31.1",
        "select-files": "^1.0.1",
        "svelte-dropzone": "^1.0.6",
        "svelte-local-storage-store": "^0.6.4",
        "svelte-markdown": "^0.4.1",
        "turndown": "^7.2.1",
        "vite": "^7.1.3"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@prisma/client",
            "@prisma/engines",
            "prisma"
        ]
    }
}
